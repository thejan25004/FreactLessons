{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended",
    "plugin:jsx-allay/recommended",
    "prettier"],

  "rules": {
    "react/prop-types":"off"
  },  
  "plugins": [],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "settings": {
    "react":{
       "version":"detect"
    },
    "import/resolver":{
      "node":{
        "extensions":[".js",".jsx"]
      }
    }
  }
}
